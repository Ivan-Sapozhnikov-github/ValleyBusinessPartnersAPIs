# API Configuration
# Copy this file to .env and fill in your actual API keys

# Google Places API Key (for reviews and competitor search)
GOOGLE_API_KEY=your_google_api_key_here

# OpenAI API Key (for o3 Deep Research)
OPENAI_API_KEY=your_openai_api_key_here

# SBA API Configuration
# Note: SBA.gov APIs may not require authentication for public data
SBA_API_KEY=your_sba_api_key_here_if_required

# Output Configuration
OUTPUT_DIR=output
